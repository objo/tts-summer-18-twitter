<div class="user_profile">
  <%= image_tag "user_icon.png", width: "100px" %>
  <br>
  <p>@<%= user.username %></p>
  <p><%= user.full_name %></p>
  <p><%= user.city %>,  <%= user.state %></p>
  <br>
  <p><%= user.bio %></p>
</div>

<div class="message_list">
  <% user.messages.each do |message| %>
    <div class="message">
      <%= image_tag "user_icon.png", width: "20px"%>
      <p><%= message.body %></p>
      <p><%= message.created_at %></p>
    </div>
  <% end %>
</div>
